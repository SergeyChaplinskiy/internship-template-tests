{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "plugins": [
      "jest",
      "jest-formatting"
    ],
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": ["error", 2],
        "no-console": "off"
    },
    "globals": {
      "jest/globals":true
    }
}